# Cell 1: Imports
import torch
import matplotlib.pyplot as plt
from tc_mmwm.models.tc_mmwm import TCMMWM
from tc_mmwm.utils.visualization import plot_rollouts

# Cell 2: Load pretrained model
model = TCMMWM.load_from_checkpoint(
    "checkpoints/tc_mmwm_best.pt"
)
model.eval()

# Cell 3: Select identical latent state
z_t = torch.load("data/example_latent_state.pt")
candidate_actions = ["push", "grasp", "move_left", "move_right"]
python
Copy code
# Cell 4: Generate counterfactual rollouts
rollouts = {}
for a in candidate_actions:
    rollouts[a] = model.counterfactual_rollout(
        z_t, action=a, horizon=6
    )

# Cell 5: Visualization (Figure 3)
plot_rollouts(
    rollouts,
    title="Counterfactual Action Rollouts from Identical Latent State"
)